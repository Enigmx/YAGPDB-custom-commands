{{if eq .StrippedID "DeclineConcluded"}}
{{$embed := structToSdict (index .Message.Embeds 0)}}
{{$embed.Set "color" 0x00FF00}}
{{try}}
{{updateMessage (complexMessageEdit "content" (print .Message.Content "\n\n" .User.Mention " has declined the report.") "buttons" (cslice  (sdict "disabled" true "label" "Accept" "style" "green" "custom_id" "forumToolsAcceptConcluded") (sdict "disabled" true "label" "Decline" "style" "red" "custom_id" "forumToolsDeclineConcluded")) "embed" (cembed $embed))}}
{{catch}}
{{editMessage $.Channel.ID $.Message.ID (complexMessageEdit "content" (print .Message.Content "\n\n" .User.Mention " has declined the report.") "buttons" (cslice  (sdict "disabled" true "label" "Accept" "style" "green" "custom_id" "forumToolsAcceptConcluded") (sdict "disabled" true "label" "Decline" "style" "red" "custom_id" "forumToolsDeclineConcluded")) "embed" (cembed $embed))}}
{{end}}
{{return}}
{{end}}
{{if eq .StrippedID "AcceptConcluded"}}
{{$channel := (index (reFindAll `\d{16,20}` .Message.Content) 1)}}
{{if (getChannelOrThread (toInt $channel))}}
{{editThread (toInt $channel) (sdict "tags" (cslice (dbGet 0 "forumTools").Value))}} 
{{sendMessage $channel (print "Done! post has been marked as " (dbGet 0 "forumTools").Value " and will close in 12 hours.")}}
{{end}}
{{$embed := structToSdict (index .Message.Embeds 0)}}
{{$embed.Set "color" 0x00FF00}}
{{try}}
{{updateMessage (complexMessageEdit "content" (print .Message.Content "\n\n" .User.Mention " has accepted the report and marked the post as " (dbGet 0 "forumTools").Value ".") "buttons" (cslice  (sdict "disabled" true "label" "Accept" "style" "green" "custom_id" "forumToolsAcceptConcluded") (sdict "disabled" true "label" "Decline" "style" "red" "custom_id" "forumToolsDeclineConcluded")) "embed" (cembed $embed))}}
{{catch}}
{{editMessage $.Channel.ID $.Message.ID (complexMessageEdit "content" (print .Message.Content "\n\n" .User.Mention " has accepted the report and marked the post as " (dbGet 0 "forumTools").Value ".") "buttons" (cslice  (sdict "disabled" true "label" "Accept" "style" "green" "custom_id" "forumToolsAcceptConcluded") (sdict "disabled" true "label" "Decline" "style" "red" "custom_id" "forumToolsDeclineConcluded")) "embed" (cembed $embed))}}
{{end}}
{{return}}
{{end}}
{{if eq .StrippedID "ModifyTags"}}
{{editThread .Channel.ID (sdict "tags" .Values)}}
{{try}}
{{updateMessage (complexMessageEdit "content" "Done! modified this post's tags.")}}
{{catch}}
{{editMessage $.Channel.ID $.Message.ID  (complexMessageEdit "content" "Done! modified this post's tags.")}}
{{end}}
{{return}}
{{end}}
